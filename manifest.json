{
   "manifest_version": 2,

   "name": "APEX Form Filler",
   "description": "Use to fill out long winded APEX pages with test data",
   "version": "1.0",

   "browser_action": {
      "default_icon": "apex.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [{
    "js": ["moment.min.js", "jquery.min.js", "content.js"],
    "run_at": "document_end"
  }],
  "permissions": [
    "notifications"
  ]
}